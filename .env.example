# Environment Variables - 실제 값으로 채워서 사용하세요

# Deployment Environment
# 배포 환경 구분 (development, staging, production)
ENVIRONMENT=development

# Azure Container Registry (배포용)
CONTAINER_REGISTRY_NAME=
RESOURCE_GROUP=etf-agent-rg
LOCATION=koreacentral

# Azure Container App (프로덕션 배포용)
CONTAINER_APP_NAME=etf-agent-app
ENVIRONMENT_NAME=etf-agent-env

# Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING=

# Azure Cosmos DB
COSMOS_ENDPOINT=
# Azure AD (RBAC) 인증 사용 시 COSMOS_KEY 주석 처리 (권장)
COSMOS_KEY=
COSMOS_DATABASE_NAME=etf-agent
COSMOS_CONTAINER_NAME=etf-data
# partition key = /symbol

# Cosmos DB Account Name (GitHub Actions에서 네트워크 ACL 설정용)
COSMOS_ACCOUNT_NAME=

# OpenAI (Semantic Kernel)
# Azure OpenAI 사용 시 아래 값은 선택적입니다
OPENAI_API_KEY=
OPENAI_ORG_ID=

# Alpha Vantage API (yfinance fallback용, 선택적)
ALPHA_VANTAGE_KEY=

# Azure OpenAI (Alternative)
# Azure OpenAI를 사용하면 OPENAI_API_KEY 불필요
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_DEPLOYMENT_NAME=
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# FastAPI
API_HOST=0.0.0.0
API_PORT=8000

# React Frontend
REACT_APP_API_URL=http://localhost:8000
